{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "LanceDB Viewer",
  "version": "1.0.0",
  "identifier": "dev.local.lancedb-viewer",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1422",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "LanceDB Viewer",
        "width": 1280,
        "height": 800,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": [
      "nsis"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}